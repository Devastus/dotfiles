#define _color0 #1a1b26
#define _color1 #F7768E

! Sensible defaults
xterm*termName: xterm-256color
xterm*font: monospace
xterm*faceName: Hack Nerd Font Mono:size=11
xterm*boldMode: false
xterm*loginShell: true
xterm*vt100*geometry: 90x34
xterm*saveLines: 2000
xterm*charClass: 33:48,35:48,37:48,43:48,45-47:48,64:48,95:48,126:48
xterm*locale: false
xterm*utf8: true
xterm*scrollTtyOutput: false
xterm*scrollKey: true
xterm*scrollBar: false
xterm*rightScrollBar: false
xterm*bellIsUrgent: true
xterm*metaSendsEscape: true

! Keybinds
xterm*translations: #override \n\
    Ctrl Shift <Key>N: scroll-back(1, halfpage) \n\
    Ctrl Shift <Key>T: scroll-forw(1, halfpage) \n\
    Ctrl Shift <Key>C: copy-selection(CLIPBOARD) \n\
    Ctrl Shift <Key>V: insert-selection(CLIPBOARD)

!Theme Tokyo night
*foreground: #a9b1d6
*background: #1a1b26
*cursorColor: #a9b1d6
*borderColor: #1a1b26
xterm*background: #1a1b26
xterm*foreground: #a9b1d6

! black
xterm*color0:     #1a1b26
xterm*color8:     #4e5173

! red
xterm*color1:     #F7768E
xterm*color9:     #F7768E

! green
xterm*color2:     #9ECE6A
xterm*color10:    #9ECE6A

! yellow
xterm*color3:     #E0AF68
xterm*color11:    #E0AF68

! blue
xterm*color4:     #7AA2F7
xterm*color12:    #7AA2F7

! magenta
xterm*color5:     #9a7ecc
xterm*color13:    #9a7ecc

! cyan
xterm*color6:     #4abaaf
xterm*color14:    #4abaaf

! white
xterm*color7:     #acb0d0
xterm*color15:    #acb0d0
